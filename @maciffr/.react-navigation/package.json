{
  "name": "@maciffr/react-navigation",
  "version": "1.7.51",
  "scripts": {
    "clean-build": "rm -rf ./lib",
    "clean-test": "rm -rf ./coverage",
    "clean-dep": "rm -rf ./node_modules",
    "clean": "npm run clean-build & npm run clean-test & npm run clean-dep",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage --ci --testResultsProcessor=\"jest-sonar-reporter\" --reporters=default --reporters=\"jest-junit\" --silent --no-cache"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/src/**/*.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "dependencies": {
    "react-router-dom": "^6.9.0"
  },
  "exports": {
    "./*": "./lib/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "lib/*",
    "src/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@maciffr/react": "^1.7.51"
  }
}
